{
  "name": "@atmyapp/react",
  "description": "React client for At My App",
  "license": "MIT",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ama": "./dist/cli/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/At-My-App/react.git"
  },
  "keywords": [
    "react",
    "typescript",
    "cli"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rmdir /s /q dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:migrate": "node ./dist/cli/test-migrate.js"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.5.0",
    "fast-glob": "^3.3.3",
    "react": ">=16.8.0",
    "ts-morph": "^25.0.0",
    "typescript": "5.3.3",
    "typescript-json-schema": "^0.65.1",
    "zod": ">=3.0.0"
  },
  "overrides": {
    "typescript-json-schema": {
      "typescript": "$typescript"
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/react": "^18.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6"
  },
  "resolutions": {
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "zod": ">=3.0.0"
  }
}
